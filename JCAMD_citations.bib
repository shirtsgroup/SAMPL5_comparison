@article{Zhou2013,
abstract = {The synthetic host cucurbit[7]uril (CB[7]) binds aromatic guests or metal complexes with ultrahigh affinity compared with that typically displayed in protein-ligand binding. Due to its small size, CB[7] serves as an ideal receptor-ligand system for developing computational methods for molecular recognition. Here, we apply the recently developed variational implicit-solvent model (VISM), numerically evaluated by the level-set method, to study hydration effects in the high-affinity binding of the B2 bicyclo[2.2.2]octane derivative to CB[7]. For the unbound host, we find that the host cavity favors the hydrated state over the dry state due to electrostatic effects. For the guest binding, we find reasonable agreement to experimental binding affinities. Dissection of the individual VISM free-energy contributions shows that the major driving forces are water-mediated hydrophobic interactions and the intrinsic (vacuum) host-guest van der Waals interactions. These findings are in line with recent experiments and molecular dynamics simulations with explicit solvent. It is expected that the level-set VISM, with further refinement on the electrostatic descriptions, can efficiently predict molecular binding and recognition in a wide range of future applications.},
annote = {So pretty confusing, but here's what I got...

        
Used implicit solvent model called VISM, which for now requires host and guest molecules to be rigid (no fluctuations) because incorporates surface free energy and solvation boundaries into solvent model.  Also, when solve for solvation shell, can have two energy minimizing conformations of water surrounding the solute molecules.  Thus, to get both solutions, started simulations with either "loose" or "tight" solvation shells.  Found this mattered (you got two different solutions) for the host alone, but not for the complex.  This indicates that waters are very free to move in and out of the host when no ligand is bound, but solvation is essentially fixed when the ligand is bound.

        
Concerning binding free energy, found that surface energy of solvation and host-guest interactions strongly favor binding, while electrostatic vdW parts of the solvation free energy changes disfavor binding (see highlighted conclusion).},
author = {Zhou, Shenggao and Rogers, Kathleen E and de Oliveira, C\'{e}sar Augusto F and Baron, Riccardo and Cheng, Li-Tien and Dzubiella, Joachim and Li, Bo and McCammon, J Andrew},
file = {:C$\backslash$:/Users/Jacob Monroe/Documents/Shirts' Lab/Molecular Containers, 2013/Papers/Zhou, 2013, Variational implicit-solvent modelling of host-guest binding.pdf:pdf},
issn = {1549-9618},
journal = {J. Chem. Theory Comput.},
keywords = {CB7},
mendeley-tags = {CB7},
month = sep,
number = {9},
pages = {4195--4204},
pmid = {24039554},
title = {{Variational Implicit-Solvent Modeling of Host-Guest Binding: A Case Study on Cucurbit[7]uril|}},
volume = {9},
year = {2013},
}

