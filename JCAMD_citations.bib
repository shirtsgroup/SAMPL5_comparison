issn = {1573-4951},
journal = {J. Comput. Aided. Mol. Des.},
keywords = {Molecular Containers,binding affinity \'{a} mm,docking \'{a} glide,gbsa \'{a} mm,linear interaction energy \'{a},pbsa \'{a}},
mendeley-tags = {Molecular Containers},
month = may,
